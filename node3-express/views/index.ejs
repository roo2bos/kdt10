        <% console.log(btns) %>
        <% for (let btn of btns){ %>
            <button class="btn btn-info"><%= btn %></button>
        <% } %>

        <% if(!isLogin){ %>
            <div>로그인이 되어있지 않습니다</div>
        <% }else{ %>
            <div>로그인이 되었습니다</div>
        <% } %>
        <%= me.name %>
        <%= me.msg %>
    <%- include('./component/contentFoot')  %>
</body>
</html>